{
  "name": "speaker-prebuilt",
  "version": "0.5.5",
  "license": "(MIT AND LGPL-2.1-only)",
  "description": "Output PCM audio data to the speakers, prebuilt version",
  "author": "Yuki Akai",
  "repository": "TooTallNate/node-speaker",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "standard && node-gyp rebuild --mpg123-backend=dummy && mocha --reporter spec",
	"prebuild": "npx @mapbox/node-pre-gyp configure build --fallback-to-build",
    "publish": "npx @mapbox/node-pre-gyp package publish --registry=https://npm.pkg.github.com"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "buffer-alloc": "^1.1.0",
    "debug": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.14",
    "mocha": "^6.2.2",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "standard": "^14.3.1"
  },
  "binary": {
    "module_name": "speaker-prebuilt",
    "module_path": "./build/release/",
    "remote_path": "./{version}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  },
  "engines": {
    "node": ">=8.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/node-speaker-prebuilt.git"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/node-speaker-prebuilt/issues"
  },
  "homepage": "https://github.com/yukiakai212/node-speaker-prebuilt#readme",
  "keywords": [
    "pcm",
    "prebuilt",
    "audio",
    "sound",
    "music",
    "output",
    "speaker",
    "headphone",
    "alsa",
    "coreaudio",
    "openal",
    "sdl",
    "portaudio",
    "jack",
    "oss",
    "pulse",
    "mpg123"
  ]
}
