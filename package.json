{
  "name": "speaker-prebuilt",
  "version": "1.5.40",
  "license": "(MIT AND LGPL-2.1-only)",
  "description": "Output PCM audio data to the speakers, prebuilt version",
  "author": "Yuki Akai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukiakai212/node-speaker-prebuilt.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "standard && node-gyp rebuild --mpg123-backend=dummy && mocha --reporter spec",
    "postinstall": "node ./scripts/extract-binary.js"
  },
  "files": [
    "index.js",
    "index.d.js",
    "lib/",
    "build/stage/*.tar.gz",
    "build/Release/**",
    "README.md"
  ],
  "dependencies": {
    "bindings": "^1.3.0",
    "buffer-alloc": "^1.1.0",
    "debug": "^4.0.0",
    "tar": "^7.4.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@types/node": "^12.12.14",
    "mocha": "^6.2.2",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">=8.6"
  },
  "bugs": {
    "url": "https://github.com/yukiakai212/node-speaker-prebuilt/issues"
  },
  "binary": {
    "module_name": "speaker-binaries",
    "module_path": "./build/Release",
    "host": "https://npm.pkg.github.com/yukiakai212/node-speaker-prebuilt/"
  },
  "homepage": "https://github.com/yukiakai212/node-speaker-prebuilt#readme",
  "keywords": [
    "pcm",
    "prebuilt",
    "audio",
    "sound",
    "music",
    "output",
    "speaker",
    "headphone",
    "alsa",
    "coreaudio",
    "openal",
    "sdl",
    "portaudio",
    "jack",
    "oss",
    "pulse",
    "mpg123"
  ]
}
